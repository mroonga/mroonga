require_relative "../mroonga-package-task"

class MariaDB104MroongaPackageTask < MroongaPackageTask
  def initialize
    super("mariadb-10.4")
  end

  def enable_apt?
    false
  end

  def enable_ubuntu?
    false
  end

  def yum_targets_default
    [
      "almalinux-8",
      "centos-7",
    ]
  end
end

task = MariaDB104MroongaPackageTask.new
task.define
