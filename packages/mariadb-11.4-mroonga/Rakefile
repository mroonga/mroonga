require_relative "../mroonga-package-task"

class MariaDB114MroongaPackageTask < MroongaPackageTask
  def initialize
    super("mariadb-11.4-mroonga")
  end

  def enable_apt?
    false
  end

  def enable_ubuntu?
    false
  end

  def yum_targets_default
    [
      "almalinux-8",
      "almalinux-9",
      "almalinux-10",
    ]
  end
end

task = MariaDB114MroongaPackageTask.new
task.define
