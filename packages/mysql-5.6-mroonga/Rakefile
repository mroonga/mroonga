require_relative "../mroonga-package-task"

class MySQL56MroongaPackageTask < MroongaPackageTask
  def initialize
    super("mysql-5.6")
    @rpm_package = "mysql56-community-mroonga"
  end

  def enable_apt?
    false
  end

  def yum_targets_default
    [
      "centos-6",
      "centos-7",
    ]
  end
end

task = MySQL56MroongaPackageTask.new
task.define
