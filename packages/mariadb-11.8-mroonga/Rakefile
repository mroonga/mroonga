require_relative "../mroonga-package-task"

class MariaDB118MroongaPackageTask < MroongaPackageTask
  def initialize
    super("mariadb-11.8-mroonga")
  end

  def apt_targets_default
    [
      "debian-trixie",
    ]
  end

  def enable_ubuntu?
    false
  end

  def yum_targets_default
    [
      "almalinux-8",
      "almalinux-9",
      "almalinux-10",
    ]
  end
end

task = MariaDB118MroongaPackageTask.new
task.define
