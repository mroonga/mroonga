require_relative "../mroonga-package-task"

class MariaDB105MroongaPackageTask < MroongaPackageTask
  def initialize
    super("mariadb-10.5")
  end

  def enable_apt?
    false
  end

  def enable_ubuntu?
    false
  end

  def yum_targets_default
    [
      "centos-7",
      "centos-8",
    ]
  end
end

task = MariaDB105MroongaPackageTask.new
task.define
