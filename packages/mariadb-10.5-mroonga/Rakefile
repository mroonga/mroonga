require_relative "../mroonga-package-task"

class MariaDB105MroongaPackageTask < MroongaPackageTask
  def initialize
    super("mariadb-10.5")
  end

  def apt_targets_default
    [
      "debian-bullseye",
    ]
  end

  def enable_ubuntu?
    false
  end

  def yum_targets_default
    [
      "almalinux-8",
      "almalinux-9",
      "amazon-linux-2",
      "centos-7",
    ]
  end
end

task = MariaDB105MroongaPackageTask.new
task.define
