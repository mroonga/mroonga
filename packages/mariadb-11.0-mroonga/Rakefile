require_relative "../mroonga-package-task"

class MariaDB110MroongaPackageTask < MroongaPackageTask
  def initialize
    super("mariadb-11.0")
  end

  def enable_apt?
    false
  end

  def enable_ubuntu?
    false
  end

  def yum_targets_default
    [
      "almalinux-8",
      "almalinux-9",
      "centos-7",
    ]
  end
end

task = MariaDB110MroongaPackageTask.new
task.define
