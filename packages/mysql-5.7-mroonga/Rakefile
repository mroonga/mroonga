require_relative "../mroonga-package-task"

class MySQL57MroongaPackageTask < MroongaPackageTask
  def initialize
    super("mysql57-community")
  end

  def enable_apt?
    false
  end

  def yum_targets_default
    [
      "centos-6",
      "centos-7",
    ]
  end
end

task = MySQL57MroongaPackageTask.new
task.define
