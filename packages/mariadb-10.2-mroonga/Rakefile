require_relative "../mroonga-package-task"

class MariaDB102MroongaPackageTask < MroongaPackageTask
  def initialize
    super("mariadb-10.2")
  end

  def enable_apt?
    false
  end

  def yum_targets_default
    [
      "centos-6",
      "centos-7",
    ]
  end
end

task = MariaDB102MroongaPackageTask.new
task.define
