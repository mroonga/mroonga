require_relative "../mroonga-package-task"

class MariaDB106MroongaPackageTask < MroongaPackageTask
  def initialize
    super("mariadb-10.6")
  end

  def apt_targets_default
    [
      "ubuntu-jammy",
    ]
  end

  def ubuntu_targets_default
    [
      ["jammy", "22.04"],
    ]
  end

  def yum_targets_default
    [
      "almalinux-8",
      "almalinux-8-aarch64",
      "centos-7",
    ]
  end
end

task = MariaDB106MroongaPackageTask.new
task.define
