require_relative "../mroonga-package-task"

class MariaDB108MroongaPackageTask < MroongaPackageTask
  def initialize
    super("mariadb-10.8-mroonga")
  end

  def enable_apt?
    false
  end

  def enable_ubuntu?
    false
  end

  def yum_targets_default
    [
      "almalinux-8",
      "almalinux-9",
      "centos-7",
    ]
  end
end

task = MariaDB108MroongaPackageTask.new
task.define
