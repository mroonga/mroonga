require_relative "../mroonga-package-task"

class MySQL56CommunityMroongaPackageTask < MroongaPackageTask
  def initialize
    super("mysql56-community")
  end

  def enable_apt?
    false
  end

  def yum_targets_default
    [
      "centos-6",
      "centos-7",
    ]
  end
end

task = MySQL56CommunityMroongaPackageTask.new
task.define
