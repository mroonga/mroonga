--source suite/groonga_include/groonga_init.inc

--disable_warnings
drop table if exists t1, t2, t3;
--enable_warnings

create table t1 (c1 int);
show table status;

create table t2 (c1 int, c2 int);
show table status;

show table status like 't2';

drop table t1,t2;

