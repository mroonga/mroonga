DROP DATABASE IF EXISTS another;
CREATE DATABASE another;
USE another;
CREATE TABLE t1 (
id int PRIMARY KEY AUTO_INCREMENT
) ENGINE=mroonga DEFAULT CHARSET utf8mb4 ROW_FORMAT=DYNAMIC;
CREATE TABLE t2 (
id int PRIMARY KEY AUTO_INCREMENT,
id_t1 int NOT NULL,
